	lmi:javaPackageName "io.github.luzzu.linkeddata.qualitymetrics.contextual.provenance.ExtendedProvenanceMetric" ;
	lmi:referTo dqm:ExtendedProvenanceMetric .

_:BasicProvenanceMetric a lmi:LuzzuMetricJavaImplementation ;
	rdfs:label "Basic Provenance Metric" ;
